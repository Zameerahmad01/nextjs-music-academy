{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "react/jsx-key": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "react/no-unescaped-entities": "off", // Disables warnings for unescaped characters like apostrophes
    "prefer-const": "warn" // Converts 'prefer-const' from error to warning
  }
}
